//- THIS IS FOR RESIDENT PROFILE 

extends resident-layout
//- this is the title of this page (template view)
block title 
    title Profile 
    
block content 
    .second_main_body
        .img_name
            img.profile_img(src="/images/Customer.png")
            h3 #{data.User_Name}
        div
            .individual
                h3 Name:
                div #{data.User_Name}
            .individual
                h3 Email address:
                div #{data.Email_Address}
            .individual
                h3 Date of Birth:
                div #{data.Date_Of_Birth.toDateString()}
            .individual
                h3 Password:
                div ********
            .individual
                h3 House Address:
                div #{data.House_Address} 
            .individual
                h3 Mobile number:
                div #{data.Phone_Number}
            button.update_button UPDATE PROFILE

        