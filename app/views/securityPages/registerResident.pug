
extends resident-layout

//- this is the title of this page (template view)
block title 
    title Register Resident

block content 
    div.code-gen-container
        div.heading Generate Code
        form.form-container
            input(type='text' name='name' placeholder="Resident name:" class="input-field")
            input(type='text' name='location' placeholder="Resident Address:" class="input-field")
            input(type='text' name='timeExpired' placeholder="Email Address:" class="input-field")
            input(type='text' name='timeExpired' placeholder="Mobile Number: " class="input-field")
            input(type='text' name='timeExpired' placeholder="Date of Birth: " class="input-field")
            input.btn(type='submit' name='generateCode' value='Submit')
