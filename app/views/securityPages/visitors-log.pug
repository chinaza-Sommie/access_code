extends security-layout
block content 
    .visitors-log-cont
        h3.visitors-subheader Visitors Log
        .search-container
            .search-icon-cont
                img(src="../assets/images/search-icon 1.png")
            input(type="text")
    table.visitors
        tr
            th resident's name
            th code value
            th visitor's name
            th Date
            th purpose of visit
            th Code status
            th entry time
            th exit time
        tr
            td Code Green
            td 012123
            td Jon Snow
            td 25/03/2024
            td Plumbing
            td Used
            td 12:00
            td 17:00